{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "strict": 0,
    "eol-last": 1,
    "no-undef": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "import/no-unresolved": 1,
    "import/named": "error",
    "semi": 1,
    "max-len": [1, 120],
    "quotes": ["error", "single"]
  }
}
